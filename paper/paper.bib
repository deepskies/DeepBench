@article{springelCosmologicalSimulationCode2005,
  title = {The Cosmological Simulation Code {{GADGET-2}}},
  author = {Springel, Volker},
  year = {2005},
  month = dec,
  journal = {Monthly Notices of the Royal Astronomical Society},
  volume = {364},
