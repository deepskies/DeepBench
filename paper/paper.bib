@article{fluxaPixelSpaceConvolution2020,
  title = {Pixel Space Convolution for Cosmic Microwave Background Experiments},
  author = {Flux{\'a}, P. and Brewer, M. K. and D{\"u}nner, R.},
  year = {2020},
  month = feb,
  journal = {J. Cosmol. Astropart. Phys.},
